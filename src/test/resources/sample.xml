<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.1.dtd">
<suite name="SampleSeleniumSuite">
    <listeners>
        <listener class-name="com.harshitbhardwaj.listener.AnnotationTransformer"/>
        <listener class-name="com.harshitbhardwaj.listener.AllureTestLifeCycleListener"/>
    </listeners>
    <test name="SampleSeleniumTests" parallel="methods" thread-count="3">
        <classes>
            <class name="com.harshitbhardwaj.tests.SampleSeleniumTests"/>
        </classes>
    </test>
</suite>